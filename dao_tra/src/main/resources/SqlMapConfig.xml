<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>

	<!-- 打印底层执行过程，默认是关闭的 -->
	<settings>
		<setting name="logImpl" value="STDOUT_LOGGING"/>
	</settings>
	
	
	<typeAliases>
		<!-- 全包扫描起别名 -->
		<package name="com.bwie.bean"/>
	</typeAliases>

	<!-- 分页插件 -->
	
	
	
	<mappers>
		<!-- 扫描到包下的文件,将类名和映射名一致的关联起来 -->
		<package name="com.bwie.dao"/>
	</mappers>
</configuration>
